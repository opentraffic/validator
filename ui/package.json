    "babel-preset-es2015": "^6.24.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-stage-1": "^6.22.0",
    "browser-sync": "^2.18.8",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.18.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.0.1",
